{
  "private": false,
  "name": "irma",
  "version": "0.35.3",
  "description": "A middleware specialized for building IR frontends",
  "main": "lib/index.js",
  "bin": {
    "irma": "main.js"
  },
  "man": "man/irma.1",
  "scripts": {
    "pegjs": "pegjs src/query-parser.pegjs lib/query-parser.pegjs",
    "manpage": "mkdir -p man && coffee src/main.coffee -m > man/irma.1",
    "prepublish": "rm -rf lib && mkdir lib && coffee --compile --output lib src && npm run pegjs && npm run diagram && npm run manpage && node mark-deps-bundled.js",
    "test": "mocha --recursive --require spec/helpers/chai.js --require coffeescript/register \"spec/**/*.{js,coffee}\"",
    "test-watch": "mocha --recursive -b -R nyan -w --watch-extensions  js,coffee --require spec/helpers/chai.js --require coffeescript/register \"spec/**/*.{js,coffee}\"",
    "server": "coffee src/cli.coffee",
    "diagram": "mkdir -p static && coffee make-diagram.coffee < src/query-parser.pegjs > static/query-syntax.html",
    "watch": "npm-watch"
  },
  "author": "Lukas Degener <l.degener@tarent.de>",
  "license": "MirOS",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldegen/irma"
  },
  "bugs": {
    "url": "https://github.com/ldegen/irma/issues"
  },
  "homepage": "https://github.com/ldegen/irma#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coffeelint": "^2.1.0",
    "csv-stringify": "0.0.8",
    "grammkit": "^0.7.1",
    "lorem-ipsum": "^1.0.1",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "rimraf": "^2.5.4",
    "rosie": "^0.4.1"
  },
  "dependencies": {
    "@l.degener/irma-config": "^1.1.0",
    "JSONStream": "^1.0.4",
    "automist": "^0.2.0",
    "bluebird": "^3.4.6",
    "cheerio": "^0.22.0",
    "coffeescript": "^2.3.2",
    "csv-stringify": "0.0.8",
    "deep-extend": "^0.6.0",
    "deepmerge": "^0.2.7",
    "elasticsearch": "^15.1.1",
    "elasticsearch-streams": "0.0.9",
    "errorhandler": "^1.3.0",
    "express": "^4.10.6",
    "express-http-proxy": "^0.11.0",
    "immutable": "^3.8.1",
    "js-yaml": "^3.13.1",
    "minimist": "^1.2.5",
    "morgan": "^1.9.1",
    "pegjs": "^0.9.0",
    "promise-ops": "^0.5.0",
    "request": "^2.51.0",
    "yargs": "^1.3.3"
  },
  "bundleDependencies": [
    "@l.degener/irma-config",
    "JSONStream",
    "automist",
    "bluebird",
    "cheerio",
    "coffeescript",
    "csv-stringify",
    "deep-extend",
    "deepmerge",
    "elasticsearch",
    "elasticsearch-streams",
    "errorhandler",
    "express",
    "express-http-proxy",
    "immutable",
    "js-yaml",
    "minimist",
    "morgan",
    "pegjs",
    "promise-ops",
    "request",
    "yargs"
  ]
}
